{
    "plugins": [
      "node_modules/better-docs/typescript"
    ],
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.ts$",
      "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "node_modules/better-docs"
    },
    "templates": {
      "better-docs": {
        "name": "My Project Documentation"
      }
    }
  }
  